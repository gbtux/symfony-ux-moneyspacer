{
    "name": "symfony/ux-moneyspacer",
    "description": "Format money input text type with spaces",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["symfony-ux"],
    "autoload": {
        "psr-4": {
            "Symfony\\UX\\Moneyspacer\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Guillaume Bordes",
            "email": "gu.bordes@gmail.com"
        }
